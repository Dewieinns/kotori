{
    "aliasColors": {},
    "bars": false,
    "datasource": "{{ datasource }}",
    "editable": true,
    "error": false,
    "fill": 1,
    "grid": {
        "leftLogBase": 1,
        "leftMax": null,
        "leftMin": null,
        "rightLogBase": 1,
        "rightMax": null,
        "rightMin": null,
        "threshold1": null,
        "threshold1Color": "rgba(216, 200, 27, 0.27)",
        "threshold2": null,
        "threshold2Color": "rgba(234, 112, 112, 0.22)",
        "thresholdLine": false
    },
    "id": {{ id }},
    "leftYAxisLabel": "{{ label_y }}",
    "legend": {
        "alignAsTable": true,
        "avg": false,
        "current": false,
        "hideEmpty": false,
        "max": false,
        "min": false,
        "rightSide": true,
        "show": true,
        "total": false,
        "values": false
    },
    "lines": true,
    "linewidth": 2,
    "links": [],
    "nullPointMode": "connected",
    "percentage": false,
    "pointradius": 5,
    "points": false,
    "renderer": "flot",
    "seriesOverrides": [],
    "span": 12,
    "stack": false,
    "steppedLine": false,
    "targets": [
        {{ targets }}
    ],
    "timeFrom": null,
    "timeShift": null,
    "title": "{{ title }} @ {{ row_title }}",
    "tooltip": {
        "shared": true,
        "value_type": "cumulative"
    },
    "type": "graph",
    "x-axis": true,
    "y-axis": true,
    "y_formats": [
        "{{ format_y }}",
        "none"
    ]
}
